{
	"name": "codekandis/json-codec",
	"description": "This library provides a codec to encode into and decode from JSON in an object oriented way.",
	"keywords": [
		"php",
		"json codec",
		"json",
		"codec",
		"encode",
		"decode"
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/json-codec/issues",
		"docs": "https://github.com/codekandis/json-codec/blob/master/README.md",
		"source": "https://github.com/codekandis/json-codec/tree/master"
	},
	"require": {
		"php": "^7.3"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"sensiolabs/security-checker": "^6",
		"phpunit/phpunit": "^8"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\JsonCodec\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\JsonCodec\\Tests\\": [
				"tests/"
			]
		}
	},
	"scripts": {
		"post-update-cmd": [
			"vendor/bin/security-checker security:check composer.lock"
		]
	}
}
